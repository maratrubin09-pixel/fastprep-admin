    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "nylas": "^7.13.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "telegram": "^2.19.8",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
