{
  "dashboard": {
    "title": "FastPrep Outbox Worker",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Outbox Processed (Total)",
        "targets": [
          {
            "expr": "rate(outbox_processed_total{status=\"done\"}[5m])",
            "legendFormat": "Done"
          },
          {
            "expr": "rate(outbox_processed_total{status=\"failed\"}[5m])",
            "legendFormat": "Failed"
          },
          {
            "expr": "rate(outbox_processed_total{status=\"retry\"}[5m])",
            "legendFormat": "Retry"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "Adapter Latency (p50, p95, p99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(adapter_latency_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(adapter_latency_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(adapter_latency_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ]
      },
      {
        "id": 3,
        "type": "singlestat",
        "title": "Failed (Last 5m)",
        "targets": [
          {
            "expr": "increase(outbox_processed_total{status=\"failed\"}[5m])"
          }
        ],
        "thresholds": "10,50"
      }
    ],
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}






